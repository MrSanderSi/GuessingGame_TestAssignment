<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="scenePane" maxHeight="90.0" maxWidth="260.0" minHeight="90.0" minWidth="260.0" prefHeight="90.0" prefWidth="260.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SceneController">
   <children>
      <Button layoutX="65.0" layoutY="50.0" maxHeight="25.0" maxWidth="60.0" minHeight="25.0" minWidth="60.0" mnemonicParsing="false" onAction="#switchToGuessingScene" prefHeight="25.0" prefWidth="60.0" text="OK" />
      <Button fx:id="exitButton" layoutX="135.0" layoutY="50.0" maxHeight="25.0" maxWidth="60.0" minHeight="25.0" minWidth="60.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="25.0" prefWidth="60.0" text="Cancel" />
      <Label layoutX="66.0" layoutY="20.0" prefHeight="18.0" prefWidth="129.0" text="Think about an animal" />
      <ImageView fitHeight="36.0" fitWidth="43.0" layoutX="14.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@questionMark.PNG" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
